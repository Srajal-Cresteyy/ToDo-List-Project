<%- include('header') -%>
  <body>
    <div class="box" id="heading">
        <h1><%= listTitle %> </h1>
    </div>
    <div class="box">
        <%for(var index=0;index<newLists.length;index++){%>
            <div class="item">
                <input type="checkbox">
                <p><%=newLists[index]%></p>
            </div>
        <%}%>
        <form class="item" action="/" method="POST">
          <input type="text" placeholder="Enter The task" name="task" autocomplete="off">
          <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>
    </div>
  </body>

<%- include('footer') -%>